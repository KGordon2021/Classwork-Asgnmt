<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="../public/styles.css">
    <title>View Orders</title>
</head>
<body>
    <%- include('../views/partials/nav.ejs') %>
<div class="adminViews">

    <h1>View of all orders recieved</h1>
    
    <table>
        <thead>
            <tr>
                <th>Student Name </th>
                <th> Books Outstanding</th>
                
            </tr>
        </thead>
        <tbody>
            
          <% records.forEach(record => { %>

            <tr>
                <td><%= record.st_fname + " " + record.st_lname %> </td>
                <td><%= record.Title_Of_Book %></td>
            </tr>
           <% }) %>
        </tbody>
    </table>
</div>


</body>
</html>